/*投资案例样式  */
#invest  {
  /* padding: 42rpx 32rpx -2rpx 32rpx;  */
  padding-top: 42rpx;
  /* padding-left: 32rpx;
  padding-right: 32rpx; */
  border-bottom: 1px solid #e6e6e6;
  margin: 0 auto;
  width: 92%;
}

#invest:last-child {
  border-bottom: 0;
}
#invest .investLogo {
  width: 20%;
}

#invest .investLogo1 {
  width: 121.68rpx;
  height: 121.68rpx;
}

#invest .investLogo1 image {
  width: 100%;
}

#invest .investRight {
  width: 80%;
  height: auto;
}

#invest .investW {
  flex-wrap: wrap;
}

#invest .investName {
  /* height: 37.44rpx; */
  margin-bottom: 22rpx;
}

#invest .investName1 {
  font-size: 15px;
  color: #333;
}

#invest .investHot {
  width: 70.2rpx;
  height: 37.44rpx;
  background: #fa855c;
  border-radius: 2px;
  font-size: 10px;
  color: #fff;
  text-align: center;
  line-height: 37.44rpx;
  margin-left: 18.72rpx;
}

#invest .investWheel {
  background: #009eff;
  border-radius: 2px;
  display: inline-block;
  padding: 2rpx 20rpx;
  height: 37.44rpx;
  color: #fff;
  font-size: 10px;
  text-align: center;
  line-height: 37.44rpx;
  margin-left: 18.72rpx;
}

#invest .investIndury {
  font-size: 12px;
  color: #666;
  height: auto;
  line-height: 38rpx;
  overflow: hidden;
  text-overflow: -o-ellipsis-lastline;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin-bottom: 24rpx;
}

#invest .investInduryone {
  display: inline-block;
}

#invest .investOther {
  font-size: 12px;
  color: #666;
}

#invest .investLeft {
  margin-right: 20rpx;
  display: inline-block;
}

#invest .investDetail {
  padding: 18rpx;
  margin-top: 20rpx;
  font-size: 12px;
  color: #40587a;
  height: auto;
  /* max-height:75rpx;  */
  line-height: 46rpx;
  overflow: hidden;
  text-overflow: -o-ellipsis-lastline;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin-bottom: 30rpx;
  background: #fafafa;
}

/*标签样式  */

#industry  {
  display: inline-block;
  padding: 0 10rpx;
  height: 56rpx;
  border-radius: 4rpx;
  font-size: 12px;
  text-align: center;
  line-height: 56rpx;
   margin-right: 30rpx;
  margin-bottom: 20rpx;
  background: rgba(0, 158, 255, 0.10);
  border: 1px solid rgba(0, 158, 255, 0.20);
  color: #009eff;
}


/*投资机构  */

#investment .investGroupBlock {
  padding-bottom: 40rpx;
  padding-top: 40rpx;
  border-bottom: 1rpx solid #e6e6e6;
}

#investment .investGroupBlock:last-child {
  border-bottom: 0;
  padding-bottom: 0;
}

#investment .investGroupLeft {
  height: 104rpx;
  width: 104rpx;
  background: blueviolet;
  margin-right: 24rpx;
}

#investment .investTitle {
  font-size: 30rpx;
  color: #333;
  line-height: 32rpx;
  text-align: left;
}

#investment .investNum {
  font-size: 24rpx;
  color: #fc703e;
  line-height: 26rpx;
  text-align: right;
}

#investment .investTitleBlock {
  margin-bottom: 20rpx;
  justify-content: space-between;
}

#investment .investIntro {
  font-size: 24rpx;
  color: #999;
  line-height: 24rpx;
  text-align: left;
  margin-bottom: 28rpx;
  width: 550rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

#investment .mainCase {
  border: 1rpx solid rgba(0, 158, 255, 0.20);
  border-radius: 4rpx;
  width: 56rpx;
  height: 28rpx;
  font-size: 20rpx;
  line-height: 32rpx;
  color: #009eff;
  text-align: center;
  margin-right: 13rpx;
}

#investment .mainCaseContent {
  font-size: 24rpx;
  color: #666;
  line-height: 36rpx;
  text-align: left;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  width: 480rpx;
  margin-bottom: 28rpx;
}

#investment .projectListBlock {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  width: 510rpx;
  background: #fafafa;
  border-radius: 4rpx;
  height: 56rpx;
  padding: 24rpx;
  font-size: 24rpx;
  color: #40587a;
  text-align: left;
}

#investment .mainCaseBox {
  margin-bottom: 28rpx;
}

#investment .tapCase {
  border: 1rpx solid #bcf1d3;
  border-radius: 4rpx;
  width: 56rpx;
  height: 28rpx;
  font-size: 20rpx;
  line-height: 32rpx;
  color: #10ad57;
  text-align: center;
  margin-right: 13rpx;
}


/*机构成员  */

 .outStyleBox {
  margin-bottom: 32rpx;
  background: #fff;
  padding: 32rpx;
}
#memberList  .swiper-box .blockTitle{
  text-align: left;
}
#memberList  .swiper-box .memberDes{
  text-align: left;
}

#memberList  .blockTitle {
  font-size: 30rpx;
  color: #333;
  line-height: 33rpx;
  text-align: left;
  margin-bottom: 40rpx;
}

#memberList  .memberInfo {
  border-bottom: 1rpx solid #e6e6e6;
  padding-bottom: 40rpx;
  margin-top:40rpx;
}

#memberList  .memberInfo:last-child {
  border: 0;
  padding-bottom:0;
}

#memberList  .memberInfoLeft {
  background: #f0f0f0;
  width: 104rpx;
  height: 104rpx;
  border-radius: 100%;
  margin-right: 24rpx;
}

#memberList .memberInfoTop {
  margin-bottom: 28rpx;
}

#memberList  .memberName {
  font-size: 30rpx;
  color: #333;
  letter-spacing: 0;
  line-height: 30rpx;
  text-align: left;
}

#memberList .memberTitle {
  border: 1rpx solid #40587a;
  border-radius: 120rpx;
  width: 54rpx;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 20rpx;
  color: #40587a;
  text-align: center;
  margin-left: 16rpx;
}

#memberList .memberCompany {
  font-size: 26rpx;
  color: #666;
  line-height: 28rpx;
  text-align: left;
  margin-bottom:26rpx;
}

#memberList .memberDes {
  background: #fafafa;
  border-radius: 4rpx;
  width: 510rpx;
  height: 64rpx;
  font-size: 24rpx;
  color: #999;
  line-height: 32rpx;
  text-align: left;
  padding: 20rpx 24rpx;
}
#memberList .memberTitleLeave{
  border: 1rpx solid #cccccc;
  border-radius: 120rpx;
  width: 54rpx;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 20rpx;
  color: #cccccc;
  text-align: center;
  margin-left: 16rpx;
}
