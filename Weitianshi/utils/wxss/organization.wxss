  -webkit-box-orient: vertical;
  margin-bottom: 24rpx;
}

#mediaNew .mediaType {
  font-size: 10px;
  color: #999;
  padding-bottom: 24rpx;
}

#mediaNew .mediaTypeRight {
  margin-right: 10rpx;
}

#mediaNew .mediaLine {
  border-bottom: 1px solid #e6e6e6;
}

