/**app.wxss**/
@import "weui.wxss";

scroll-view {
  height: 100%;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.flex {
  display: flex;
}

page {
  background-color: #f8f8f8;
  /*font-size: 16px;*/
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
}

.page__hd {
  padding: 40px;
}

.page__bd {
  padding-bottom: 40px;
}

.page__bd_spacing {
  padding-left: 15px;
  padding-right: 15px;
}

.page__ft {
  padding-bottom: 10px;
  text-align: center;
}

.page__title {
  text-align: left;
  font-size: 20px;
  font-weight: 400;
}

.page__desc {
  margin-top: 5px;
  color: #888;
  text-align: left;
  font-size: 14px;
}

/*能用蓝色按钮*/

.nextBtn {
  background: #40587a;
  position: relative;
  padding: 0 36rpx;
  color: white;
  font-size: 32rpx;
  border-radius: 200rpx;
  margin: 44rpx 0;
  line-height: 3.25;
}

.rightArrow {
  position: absolute;
  top: 37rpx;
  right: 38rpx;
  color: white;
  z-index: 999;
  width: 16px;
  height: 16px;
  display: block;
}

