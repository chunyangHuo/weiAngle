}

/*tab==0  */

.bigTitle {
  height: 216px;
  width: 100%;
  background: yellow;
  position: relative;
}

.searchImageBox {
  background: #009eff;
  width: 120rpx;
  height: 76rpx;
  position: absolute;
  top: 0;
  right: 0;
}

.searchImage {
  height: 40rpx;
  width: 40rpx;
  position: absolute;
  top: 22rpx;
  right: 37rpx;
}

.swiper-box .bigTitle .searchSth {
  text-align: left;
}

.searchSth {
  background: #fff;
  border: 1rpx solid rgba(64, 88, 122, 0.30);
  border-radius: 4rpx;
  width: 596rpx;
  height: 18rpx;
  position: absolute;
  top: 294rpx;
  left: 62rpx;
  font-size: 24rpx;
  color: #999;
  line-height: 24rpx;
  text-align: left;
  padding: 28rpx 0 30rpx 26rpx;
}

.outStyleBox {
  margin-bottom: 32rpx;
  background: #fff;
  padding: 32rpx;
}

/* 投资风格 */

.blockTitle {
  font-size: 30rpx;
  color: #333;
  line-height: 33rpx;
  text-align: left;
  margin-bottom: 16rpx;
}

.blockIntro {
  font-size: 20rpx;
  color: #999;
  line-height: 22rpx;
  text-align: left;
  margin-bottom: 40rpx;
}

.investStyleBlock {
  background: #fff;
  box-shadow: 0 0 12rpx 0 rgba(64, 88, 122, 0.10);
  border-radius: 8rpx;
  width: 220rpx;
  height: 152rpx;
  margin-bottom: 16rpx;
  margin-right: 12rpx;
}

.investStyleTitle {
  font-size: 26rpx;
  color: #333;
  line-height: 26rpx;
  padding-top: 44rpx;
  padding-bottom: 16rpx;
}

.investStyleNum {
  font-size: 22rpx;
  color: #40587a;
  line-height: 25rpx;
}

.swiper-box .outStyleBox view {
  text-align: left;
}

.swiper-box .outStyleBox .investStyleBlock view {
  text-align: center;
}

/*热门领域  */

.industryTitleBlock {
  font-size: 22rpx;
  color: #999;
  line-height: 22rpx;
  text-align: left;
}

.industryTitle {
  margin-right: 40rpx;
  margin-bottom: 32rpx;
}

.industryTitle:first-child {
  margin-right: 298rpx;
}

.industryTitle:last-child {
  margin-right: 0;
}

.industryContentBlock {
  font-size: 28rpx;
  color: #333;
  line-height: 30rpx;
  text-align: left;
  margin-bottom: 64rpx;
}

.industryContentBlock:last-child {
  margin-bottom: 0;
}

.industryContentTitle {
  width: 342rpx;
}

.industryContent {
  width: 128rpx;
}

.industryContent:last-child {
  width: 88rpx;
}

.allThing {
  font-size: 24rpx;
  color: #009eff;
  line-height: 26rpx;
  text-align: right;
}

.title {
  justify-content: space-between;
}

/*投资机构  */

.investGroupBlock {
  padding-bottom: 40rpx;
  padding-top: 40rpx;
  border-bottom: 1rpx solid #e6e6e6;
}

.investGroupBlock:last-child {
  border-bottom: 0;
  padding-bottom: 0;
}

.investGroupLeft {
  height: 104rpx;
  width: 104rpx;
  background: blueviolet;
  margin-right: 24rpx;
}

.investTitle {
  font-size: 30rpx;
  color: #333;
  line-height: 32rpx;
  text-align: left;
}

.investNum {
  font-size: 24rpx;
  color: #fc703e;
  line-height: 26rpx;
  text-align: right;
}

.investTitleBlock {
  margin-bottom: 20rpx;
  justify-content: space-between;
}

.investIntro {
  font-size: 24rpx;
  color: #999;
  line-height: 24rpx;
  text-align: left;
  margin-bottom: 28rpx;
  width: 550rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.mainCase {
  border: 1rpx solid rgba(0, 158, 255, 0.20);
  border-radius: 4rpx;
  width: 56rpx;
  height: 28rpx;
  font-size: 20rpx;
  line-height: 32rpx;
  color: #009eff;
  text-align: center;
  margin-right: 13rpx;
}

.mainCaseContent {
  font-size: 24rpx;
  color: #666;
  line-height: 36rpx;
  text-align: left;
  overflow: hidden;
  text-overflow: -o-ellipsis-lastline;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  width: 480rpx;
}

.investTime {
  background: #fafafa;
  border-radius: 4rpx;
  width: 510rpx;
  height: 56rpx;
  padding: 24rpx;
  font-size: 24rpx;
  color: #40587a;
  text-align: left;
  word-break: break-all;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.mainCaseBox {
  margin-bottom: 28rpx;
}

.tapCase {
  border: 1rpx solid #bcf1d3;
  border-radius: 4rpx;
  width: 56rpx;
  height: 28rpx;
  font-size: 20rpx;
  line-height: 32rpx;
  color: #10ad57;
  text-align: center;
  margin-right: 13rpx;
}
