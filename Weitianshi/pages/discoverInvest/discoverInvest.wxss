/*tab切换页  */

.swiper-tab {
  /* width: 88%; */
  line-height: 80rpx;
  background: #40587a;
  z-index: 999;
  padding-left: 32rpx;
}

.swiper-tab-list {
  display: inline-block;
  /* padding-left: 32rpx; */
  margin-right: 58rpx;
  font-size: 30rpx;
  color: #999;
  line-height: 62rpx;
  text-align: center;
  border: none;
}

.swiper-tab-list:last-child {
  margin-right: 0;
}

.on {
  color: white;
  text-align: center;
  font-size: 30rpx;
  line-height: 80rpx;
  border-bottom: 4rpx solid white;
}

.dropDown {
  top: 160rpx;
}

/*banner轮播图  */

.slide-image {
  width: 100%;
  height: 100%;
}

/*精选项目copy  */

.browseMeTitle {
  /*height: 90rpx;*/
