<!--My寻找案源-->
<template name="InvestInfo">
  <view class="user_invest_info" bindtap="findProjectEdit">
    <view class="flex title">
      <view class="title">
        <image class="title_icon" src="/img/icon-anyuan@3x.png"></image>
