<!--项目店铺-->
<template name="financingProject">
  <view class="user_project_info my-projectm">
    <view class="intro_title">
      <view class="flex title" catchtap="projectFinance">
        <view class="flex">
          <image class="title_icon" src="/img/icon-rongzi@3x.png"></image>
          <view>{{text}}
            <label>({{count.project_count}})</label>
          </view>
        </view>
        <view class="flex">
          <view class="seeMore">查看更多</view>
          <image class="iconEdit" wx:if="{{canEdit}}" src="/img/icon-edit@3x.png"></image>
        </view>
      </view>
    </view>
    <block wx:for="{{project_info}}" wx:key="{{item.pro_intro}}">
      <view class="user_project_frame" catchtap="financingDetail" data-id="{{item.pro_id}}">
        <view class="flex">
          <view class="pro_intro">{{item.pro_intro||item.case_name}}</view>
          <view class='is_exclusive' wx:if='{{item.is_exclusive===1}}'>独家</view>
          <view class='is_exclusive' wx:if='{{item.is_exclusive===2}}'>非独</view>
        </view>
        <block wx:for="{{item.pro_industry||item.case_industry}}" wx:key="{{item.industry_id}}" wx:for-item="pro_industry">
          <label class="noLight" wx:if="{{index < item.pro_industry.length-1}}" data-id="{{pro_industry.industry_id}}" data-vlaue="{{pro_industry.industry_name}}">{{pro_industry.industry_name}}、</label>
          <label class="noLight" wx:if="{{index == item.pro_industry.length-1}}" data-id="{{pro_industry.industry_id}}" data-vlaue="{{pro_industry.industry_name}}">{{pro_industry.industry_name}}</label>
        </block>
        <view class="flex">
          <view class="noLight" wx:if="{{item.pro_scale.scale_money}}">{{item.pro_scale.scale_money}}、</view>
          <view class="noLight" wx:if="{{item.pro_finance_stock_after}}">{{item.pro_finance_stock_after}}%、</view>
          <view class="noLight" wx:if="{{item.pro_stage.stage_name}}">{{item.pro_stage.stage_name}}、</view>
          <view class="noLight" wx:if="{{item.pro_city.area_title}}">{{item.pro_city.area_title}}</view>
        </view>
      </view>
    </block>
  </view>
</template>

<!--My投资案例-->
<template name="investProject">
  <view class="user_project_info investment-casem">
    <view class="intro_title">
      <view class="flex title" catchtap="investCase">
        <view class="flex">
          <image class="title_icon" src="/img/icon-anli@3x.png"></image>
          成功案例
          <label> ({{invest_case.length}})</label>
        </view>
        <view class="flex" wx:if="{{canEdit}}">
          <view class="seeMore">查看更多</view>
          <image class="iconEdit" src="/img/icon-edit@3x.png"></image>
        </view>
      </view>
    </view>
    <view class="invest_info_frame">
      <block wx:for="{{invest_case}}" wx:key="{{item.case_id}}">
        <view class="user_project_frame" data-id="{{item.case_id}}" data-index="{{index}}">
          <view class="flex">
            <view class="project_time" wx:if="{{item.case_deal_time}}">{{item.case_deal_time}}</view>
            <view class="project_name" wx:if="{{item.case_name}}">{{item.case_name}}</view>
            <view class="pro_sort" wx:if="{{item.stage.stage_name}}">{{item.stage.stage_name}}</view>
          </view>
          <block wx:for="{{item.industry}}" wx:key="{{item.industry_id}}" wx:for-item="pro_industry">
            <label class="noLight" wx:if="{{index < item.industry.length -1}}" data-id="{{pro_industry.industry_id}}" data-vlaue="{{pro_industry.industry_name}}">{{pro_industry.industry_name}}、</label>
            <label class="noLight" wx:if="{{index == item.industry.length-1}}" data-id="{{pro_industry.industry_id}}" data-vlaue="{{pro_industry.industry_name}}">{{pro_industry.industry_name}}</label>
          </block>
          <view class="flex s_pro_sorts">
            <view class="pro_sorts flex">
              <view class="noLight" wx:if="{{item.case_money}}">{{item.case_money}}万、</view>
              <view class="noLight" wx:if="{{item.stage.stage_name}}">{{item.stage.stage_name}}</view>
            </view>
          </view>
        </view>
      </block>
    </view>
  </view>
</template>

<!--项目融资列表 && 投资案例列表-->
<template name="projectList">
  <view class="weui-panel__bd" style="background:white;margin:16px">
    <view class="weui-media-box weui-media-box_text my_detail" data-id='{{myProject.project_id}}' data-index="{{index}}" data-id='{{myProject.case_id}}'>
      <view class="title" style="text-align:left;">{{myProject.pro_intro||myProject.case_name}}</view>
