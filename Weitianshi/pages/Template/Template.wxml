      <block wx:for="{{resource.res_find}}" wx:key="{{item.resource_id}}">
        <label class="investStyle" wx:if="{{index < resource.res_find.length-1}}" data-id="{{item.resource_id}}" data-value="{{item.resource_name}}">{{item.resource_name}}、</label>
        <label class="investStyle" wx:if="{{index ==  resource.res_find.length-1}}" data-id="{{item.stage_id}}" data-value="{{item.stage_name}}">{{item.resource_name}}</label>
