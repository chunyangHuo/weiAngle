/* pages/myProject/scanCode/scanCode.wxss */

page {
  background-color: white;
  width: 100%;
  height: 100%;
  position: relative;
}

.bigBox {
  margin: 32rpx 32rpx 0 32rpx;
}

.followPerson, .brightPoint, .material, .corePerson, .information, .milestone,
.signFA {
  background: #fafafa;
  border-radius: 2px;
  padding: 32rpx 24rpx 32rpx 24rpx;
  margin-bottom: 20rpx;
}

.title {
  font-size: 14px;
  color: #333;
  line-height: 14px;
  text-align: left;
}

.titlePic {
  width: 28rpx;
  height: 28rpx;
  padding-right: 14rpx;
  display: inline-block;
}

.titleRight {
  font-size: 24rpx;
  color: #666;
  line-height: 24rpx;
  text-align: right;
  width: 80%;
}

.titleRightBP {
  font-size: 24rpx;
  color: #666;
  line-height: 24rpx;
  text-align: right;
  width: 74%;
}

.contentAll {
  margin-top: 46rpx;
}

.contentBox {
  margin-bottom: 20rpx;
}

.personName, .position, .company {
  font-size: 26rpx;
  color: #333;
  line-height: 26rpx;
  text-align: left;
  margin-right: 16rpx;
}

.position {
  color: #666;
}

.company {
  color: #999;
}

.time {
  font-size: 24rpx;
  color: #666;
  line-height: 24rpx;
  text-align: left;
}

.star {
  margin-bottom: 26rpx;
}

.brightContent {
  height: 76rpx;
  font-size: 26rpx;
  color: #666;
  line-height: 39rpx;
  text-align: left;
}

.tips {
  font-size: 24rpx;
  color: #40587a;
  letter-spacing: 0;
  line-height: 42rpx;
  text-align: left;
  padding: 10rpx 16rpx;
  border: 2rpx solid #40587a;
  border-radius: 28rpx;
  width: auto;
  height: 42rpx;
  margin-bottom: 48rpx;
  margin-right: 24rpx;
}

.grayName {
  font-size: 24rpx;
  color: #999;
  line-height: 24rpx;
  text-align: left;
  margin-right: 8rpx;
  margin-bottom: 32rpx;
}

.personPosition {
  font-size: 24rpx;
  color: #666;
  letter-spacing: 0;
  line-height: 24rpx;
  text-align: left;
}

.personInfo:last-child {
  border-bottom: none;
}

.personInfo:first-child {
  padding: 24rpx 0;
}

.personInfo {
  border-bottom: 2rpx solid #e6e6e6;
  padding-top: 24rpx;
}

.personInfoList {
  margin-bottom: 16rpx;
}

.scale {
  font-size: 22rpx;
  color: #40587a;
  letter-spacing: 0;
  line-height: 22rpx;
  text-align: right;
  width: 76%;
}

.introduce {
  font-size: 22rpx;
  color: #666;
  letter-spacing: 0;
  line-height: 30rpx;
  text-align: left;
}

.proposalFinal {
  margin-bottom: 0;
}

.materialContent {
  font-size: 26rpx;
  color: #666;
  letter-spacing: 0;
  line-height: 26rpx;
  text-align: left;
}

.materialWeb {
  text-decoration: underline;
  color: #009eff;
}

.whiteContent {
  background-color: #fff;
  border: 0 solid #e6e6e6;
  border-radius: 4rpx;
  width: 100%;
  height: 176rpx;
}

.txtContent {
  margin: 62rpx 70rpx 54rpx 26rpx;
}

.textTab {
  font-size: 22rpx;
  color: #666;
  line-height: 22rpx;
  text-align: center;
}

.textNum {
  font-size: 30rpx;
  color: #333;
  line-height: 30rpx;
  text-align: center;
}

.purpose {
  margin-bottom: 48rpx;
}

.purposeTitle {
  font-size: 24rpx;
  color: #999;
  line-height: 24rpx;
  text-align: left;
  margin-bottom: 24rpx;
}

.purposeContent {
  font-size: 26rpx;
  color: #666;
  line-height: 30rpx;
  text-align: left;
}

.case_money {
  font-size: 26rpx;
  color: #333;
  line-height: 26rpx;
  text-align: left;
  width:64%;
}

.case_turn {
  border-radius: 62rpx;
  font-size: 20rpx;
  text-align: right;
  border: 1rpx solid #40587a;
  padding: 2rpx;
}
.invest{
  padding-bottom:48rpx;
  border-left:1px solid #e6e6e6;
}
.invest:last-child{
  border:none;
  padding:0;
}
.investContent {
  font-size: 22rpx;
  color: #666;
  line-height: 22rpx;
  text-align: left;
  padding-left:32rpx;
}

.milestoneBox {
  margin-bottom: 48rpx;
}

.milestoneContent:last-child {
  border-left: none;
  padding: 0;
}

.milestoneContent {
  border-left: 2rpx solid #e6e6e6;
  padding-bottom: 48rpx;
}

.case_time {
  font-size: 24rpx;
  color: #333;
  line-height: 24rpx;
  text-align: left;
  margin-right: 48rpx;
}

.case_pic {
  margin-top: -1rpx;
  width: 16rpx;
  height: 16rpx;
  margin-right: 24rpx;
  margin-left: -6rpx;
  z-index: 3;
}

.case_content {
  font-size: 24rpx;
  color: #666;
  line-height: 32rpx;
  text-align: left;
}

.signFABox {
  margin-bottom: 44rpx;
}

.conBox {
  margin: 56rpx 0 54rpx 52rpx;
}

.textFAContent {
  margin-right: 60rpx;
  text-align: center;
}

.textTab {
  margin-bottom: 16rpx;
}
