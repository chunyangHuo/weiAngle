/*加人脉*/

.addNetwork {
  background: #40587a;
  border-radius: 200rpx;
  width: 120rpx;
  height: 48rpx;
  color: #fff;
}

.addNetwork .addNet {
  font-size: 20rpx;
  color: #fff;
  line-height: 54rpx;
  display: block;
  text-align: center;
}

.netWork {
  opacity: 0.8;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #999;
  letter-spacing: 0;
  line-height: 28rpx;
  text-align: left;
}

.addPerson {
  border-radius: 200rpx;
  width: 120rpx;
  height: 50rpx;
  position: absolute;
  right: 12rpx;
  top: 38rpx;
}

.addPersonBtn {
  background: #40587a;
  font-family: PingFangSC-Regular;
  font-size: 20rpx;
  border-radius: 100px;
  color: #fff;
  padding: 14rpx 0;
  line-height: 20rpx;
  text-align: center;
  position: static;
}

.addPersonBtn::after {
  border: 0;
}

.waitPerson {
  position: absolute;
  right: 12rpx;
  font-size: 20rpx;
  color: #999;
  line-height: 20rpx;
  text-align: center;
  top: 38rpx;
  background: none;
}

.waitPersonBtn {
  background: #fff;
  border: 1rpx solid #ccc;
  border-radius: 100px;
  width: 118rpx;
  /*height: 48rpx;*/
  color: #fff;
  padding: 14rpx 0;
  line-height: 20rpx;
  text-align: center;
  position: static;
}

