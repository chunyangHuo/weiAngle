}

.swiper-box view {
  text-align: center;
}

/*发布按钮*/

.publish {
  margin: 32rpx;
  background: white;
  border-radius: 52rpx;
  padding: 24rpx 0;
}

.publish view {
  display: block;
  margin: auto;
}

.publish view:first-child {
  font-size: 28rpx;
  padding-bottom: 8rpx;
}

.publish view:nth-child(2) {
  font-size: 20rpx;
}

.weui-media-box {
  padding: 32rpx;
}

.weui-media-box__desc {
  font-size: 24rpx;
  color: #666;
  border-right: 1px solid #e0e0e0;
  padding: 0 8rpx;
}

.weui-media-box__desc:last-child {
  border: none;
}

.weui-media-box__desc:first-child {
  padding-left: 0;
}

.weui-media-box__info .publish {
  border: 1px #777 solid;
  width: 6em;
  padding: 3px 0;
  display: inline-block;
}

.box__title_in-text {
  font-size: 28rpx;
  text-align: left;
  padding-bottom: 18rpx;
}

.theOne {
  position: absolute;
  right: 48rpx;
  background: #40587a;
  color: white;
  display: inline-block;
  width: 5em;
  text-align: center;
}

.sort {
  display: flex;
}

.tip {
  border: 1px solid #e0e0e0;
  border-radius: 49px;
  font-size: 20rpx;
  padding: 4rpx 15rpx;
  margin-right: 12rpx;
  margin-top: 20rpx;
  color: #999;
}

.tip:last-child {
  border: 1px solid #e0e0e0;
}

.tips {
  justify-content: space-between;
}

