.searchBox {
  background: #fff;
  top: 0;
  left: 0;
  position: fixed;
  z-index: 99999;
  border-radius: 0;
  padding: 20rpx 32rpx;
  height: auto;
}

.searchSth {
  width: 684rpx;
  border-radius: 92rpx;
  background: #f5f5f5;
  height: 64rpx;
  line-height: 64rpx;
  font-size: 24rpx;
  color: #999;
  letter-spacing: 0;
  text-align: left;
}

.search_icon1 {
  display: block;
  line-height: 64rpx;
  margin: 14rpx 16rpx 0 30rpx;
}

/*全网项目库  */

.title {
  justify-content: space-between;
}

.detailTitle1 {
  font-size: 30rpx;
  color: #333;
  line-height: 42rpx;
  text-align: left;
  font-weight: bold;
  padding: 32rpx 0;
}

.detailTitle {
  font-size: 30rpx;
  color: #333;
  line-height: 42rpx;
  text-align: left;
  font-weight: bold;
  padding: 32rpx 0;
  padding-bottom: 8rpx;
}

.allNumber {
  font-size: 24rpx;
  color: #3378e0;
  text-align: right;
  position: relative;
  top: 36rpx;
}

.allNetProject, .projectSpecial, .newProject {
  padding: 0 32rpx;
  background: #fff;
  margin-bottom: 32rpx;
}

/*项目特色  */

.projectSpecial {
  padding-bottom: 20rpx;
}

.blockIntro {
  font-size: 20rpx;
  color: #999;
  line-height: 22rpx;
  text-align: left;
  margin-bottom: 40rpx;
}

.projectSpecialBox {
  flex-wrap: wrap;
}

.projectSpecialBlock {
  background: #fff;
  box-shadow: 0 0 12rpx 0 rgba(64, 88, 122, 0.10);
  border-radius: 8rpx;
  width: 220rpx;
  height: 152rpx;
  margin-bottom: 16rpx;
  margin-right: 9rpx;
  text-align: center;
}

.projectSpecialTitle {
  font-size: 26rpx;
  color: #333;
  line-height: 26rpx;
  padding-top: 44rpx;
  padding-bottom: 16rpx;
}

.projectSpecialWord {
  color: #999;
  font-weight: lighter;
  font-size: 22rpx;
  line-height: 25rpx;
}

.projectSpecialNum {
  font-size: 22rpx;
  color: #40587a;
  line-height: 25rpx;
  font-weight: bold;
}
