/*tab切换页  */

.swiper-tab {
  width: 64%;
  line-height: 80rpx;
  background: #40587a;
  z-index: 999;
}

.swiper-tab-list {
  display: inline-block;
  width: 20%;
  font-size: 26rpx;
  color: #999;
  line-height: 62rpx;
  text-align: center;
  border: none;
  /* margin-left:30%; */
}

.swiper-tab {
  padding-left: 36%;
}

.on {
  color: white;
  text-align: center;
  font-size: 26rpx;
  line-height: 80rpx;
  border-bottom: 4rpx solid white;
}

.dropDown {
  top: 160rpx;
}

/*banner轮播图  */

.slide-image {
  width: 100%;
  height: 100%;
}

/*精选项目copy  */

.browseMeTitle {
  /*height: 90rpx;*/
  margin: 32rpx 32rpx 32rpx 32rpx;
}

.browseMeTitle .num {
  font-size: 24rpx;
  color: #666;
  line-height: 24rpx;
  text-align: right;
}

.title {
  font-size: 26rpx;
  color: #333;
  line-height: 26rpx;
  text-align: left;
}

.title .block {
  background: #40587a;
  width: 4rpx;
  height: 24rpx;
  margin-right: 12rpx;
}

/*人脉王浮标  */

.competitor {
  position: fixed;
  bottom: 132rpx;
  right: 0;
  background: #fff;
  border: 0 solid #e0e6ed;
  box-shadow: 0 0 2px 0 rgba(64, 88, 122, 0.10);
  width: 166rpx;
  height: 33px;
  font-family: MicrosoftYaHei;
  font-size: 28rpx;
  color: #40587a;
  letter-spacing: 0;
  line-height: 66rpx;
  text-align: right;
  border-radius: 35rpx 0 0 35rpx;
  padding-right: 28rpx;
}

/*模态框  */

.modalBox {
  background: rgba(0, 0, 0, 0.5);
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 9999999;
}

.contactModalBox {
  height: 652rpx;
  width: 506rpx;
  background-color: white;
  margin-top: 206rpx;
  margin-left: 79rpx;
  padding: 40rpx 32rpx 32rpx 32rpx;
  position: relative;
  z-index: 99999999;
  border-radius: 4rpx;
}

.modalTitle {
  font-size: 32rpx;
  color: #333;
  line-height: 32rpx;
  text-align: center;
  margin-bottom: 16rpx;
}

.modalContent {
  border: 1rpx solid #e6e6e6;
  /* width:504rpx; */
  height: 258rpx;
  margin-bottom: 48rpx;
  margin-right: 0;
  outline: none;
  padding-top: 20rpx;
  padding-left: 24rpx;
  font-size: 28rpx;
  color: #999;
  line-height: 28rpx;
}

.modalTips {
  font-size: 28rpx;
  color: #666;
  line-height: 40rpx;
  text-align: center;
  padding: 0 28rpx;
}

.btn {
  background: #40587a;
  border-radius: 6rpx;
  width: 506rpx;
  height: 88rpx;
  font-size: 32rpx;
  color: #fff;
  line-height: 88rpx;
  text-align: center;
  margin-top: 72rpx;
}

.close {
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  top: 185rpx;
  right: 78rpx;
  z-index: 999999999;
}

.aPic {
  height: 250rpx;
  width: 276rpx;
  margin: 96rpx 0 62rpx 118rpx;
}

.newBox {
  color: #333;
}

.searchNothing {
  font-size: 24rpx;
  color: #999;
  letter-spacing: 0;
  line-height: 24rpx;
  text-align: center;
  margin-top: 252rpx;
}

.searchLine {
  margin-bottom: 72rpx;
  height: 2rpx;
  margin-left:32rpx;
  margin-right: 32rpx;
  border-bottom:2rpx dashed #999;
  width: 90%;
}
